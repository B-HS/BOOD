datasource db {
  provider = "sqlite"
  url      = "file:./food.db"
}

generator client {
  provider = "prisma-client-js"
}

model Food {
  fid       Int      @id @default(autoincrement())
  am        String
  pm        String
  date      DateTime
  insertdate DateTime @default(now())
}

model Tokens {
  tid        Int      @id @default(autoincrement())
  token      String
  isAm       Boolean
  isPm       Boolean
  insertdate DateTime @default(now())
}
